<?xml version="1.0" encoding="UTF-8"?>
<configuration>
  <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
    <encoder>
      <Pattern>%d %level [%thread] %logger - %m%n</Pattern>
    </encoder>
  </appender>
  <root level="${root-level:-ERROR}">
    <appender-ref ref="CONSOLE"/>
  </root>
</configuration>


